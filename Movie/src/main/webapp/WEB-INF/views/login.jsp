<script type="text/javascript">
	window.onload = function(){
		var message = '${msg}'; 
		if(message!=""){
			alert(message);
		}
	}
	 
	
	function clickmember(){
		document.getElementById("aTabMember").className ="on";
		document.getElementById("aTabNonMember").className ="";
		document.getElementById("tabNonMemberContent").className="tab_content"
		document.getElementById("tabMemberContent").className="tab_content on"
	}
	function clicknonmember(){
		document.getElementById("aTabMember").className ="";
		document.getElementById("aTabNonMember").className ="on";
		document.getElementById("tabNonMemberContent").className="tab_content on"
		document.getElementById("tabMemberContent").className="tab_content"
		
		
	}
	function member(){
		var id=document.getElementById("mb_id").value;
		var password=document.getElementById("mb_password").value;
		if(id==""){
			$(".pop_wrap").css("display","");
			$(".point_txt").text('아이디를 입력하세요.');
		}
		else if(password==""){
			$(".pop_wrap").css("display","");
			$(".point_txt").text('비밀번호를 입력하세요.');
		}
		else{
			document.form1.action = "memberlogincheck";
			document.form1.submit();
		}
	}
	function nonMember(){
		var id=document.getElementById("nmb_name").value;
		var password=document.getElementById("nmb_password").value;
		var passwordCheck=document.getElementById("nmb_passwordCheck").value;
		if($(".agree").is(":checked")){
			if(password!=passwordCheck){
				$(".pop_wrap").css("display","");
				$(".point_txt").text('비밀번호가 다릅니다.');
			}
			else if(id !="" && password!="" && passwordCheck!=""){
				document.form1.action = "nonmemberinsert";
				document.form1.submit(); 
			}
			else if(id==""){
				$(".pop_wrap").css("display","");
				$(".point_txt").text('이름을 입력하세요.');
			}
			else{
				$(".pop_wrap").css("display","");
				$(".point_txt").text('비밀번호를 입력하세요.');
			}
				
		}
		else{
			$(".point_txt").text('개인정보 취급에 대한 동의를 해야 비회원 로그인 가능합니다.');
			$(".pop_wrap").css("display","");
		}
	}
	function popclose(){
			$(".pop_wrap").css("display","none");
	}
	/* alert('아이디와 비밀번호를 확인해 다시 시도해주시기 바랍니다.'); */
	


</script>

